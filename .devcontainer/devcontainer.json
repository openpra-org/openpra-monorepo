{
  "name": "openpra-monorepo-dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "runArgs": ["-p5672:5672", "-p15672:15672", "-p27017:27017"],
  "workspaceFolder": "/workspaces/openpra-monorepo",
  "hostRequirements": {
    "memory": "8gb",
    "cpus": 4,
    "storage": "32gb"
  },
  "postStartCommand": "/entrypoint.sh",
  "shutdownAction": "none"
}