# exec storage dead letter exchange
MQ_EXEC_STORAGE_DEAD_LETTER_EXCHANGE_ID=global_exchange
MQ_EXEC_STORAGE_DEAD_LETTER_EXCHANGE_TYPE=direct
MQ_EXEC_STORAGE_DEAD_LETTER_EXCHANGE_DURABLE=true
MQ_EXEC_STORAGE_DEAD_LETTER_BINDING_KEY=exec_storage_dead_queue_key
