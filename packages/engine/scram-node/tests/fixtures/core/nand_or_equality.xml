<?xml version="1.0"?>
<!--
This input tests if NAND(A, -A) is equal to OR(A, -A) which should be unity.
-->
<opsa-mef>
  <define-fault-tree name="NAND_OR_EQUALITY">
    <define-gate name="TopEvent">
      <nand>
        <basic-event name="A"/>
        <gate name="NotGate"/>
      </nand>
    </define-gate>
    <define-gate name="NotGate">
      <not>
        <basic-event name="A"/>
      </not>
    </define-gate>
    <define-basic-event name="A">
      <float value="0.1"/>
    </define-basic-event>
    <define-basic-event name="B">
      <float value="0.2"/>
    </define-basic-event>
    <define-basic-event name="C">
      <float value="0.3"/>
    </define-basic-event>
  </define-fault-tree>
</opsa-mef>
