<?xml version="1.0"?>
<opsa-mef>
  <define-fault-tree name="TwoTrains">
    <define-gate name="TopEvent">
      <and>
        <event name="TrainOne"/>
        <event name="TrainTwo"/>
      </and>
    </define-gate>
    <define-gate name="TrainOne">
      <or>
        <event name="ValveOne"/>
        <event name="PumpOne"/>
      </or>
    </define-gate>
    <define-gate name="TrainTwo">
      <or>
        <event name="ValveTwo"/>
        <event name="PumpTwo"/>
      </or>
    </define-gate>
    <define-CCF-group name="Pumps" model="alpha-factor">
      <members>
        <basic-event name="PumpOne"/>
        <basic-event name="PumpTwo"/>
        <basic-event name="ValveOne"/>
        <basic-event name="ValveTwo"/>
      </members>
      <distribution>
        <float value="0.1"/>
      </distribution>
      <factors> <!-- The order of factors is flexible. -->
        <factor level="3">
          <float value="0.1"/>
        </factor>
        <factor level="1">
          <float value="0.7"/>
        </factor>
        <factor level="2">
          <float value="0.1"/>
        </factor>
      </factors>
    </define-CCF-group>
  </define-fault-tree>
</opsa-mef>
