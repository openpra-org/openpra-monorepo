<?xml version="1.0"?>
<!--
This input contains expression that have a cycle through parameters.
-->

<opsa-mef>
  <define-fault-tree name="TopEvent">
    <define-gate name="TopEvent">
      <or>
        <house-event name="BooleanConstant"/>
        <basic-event name="CyclicFormula"/>
      </or>
    </define-gate>
    <define-house-event name="BooleanConstant">
      <constant value="true"/>
    </define-house-event>
    <define-basic-event name="CyclicFormula">
      <parameter name="Head"/>
    </define-basic-event>
    <define-parameter name="Head">
      <parameter name="Middle"/>
    </define-parameter>
    <define-parameter name="Middle">
      <exponential>
        <parameter name="Head"/>
        <system-mission-time/>
      </exponential>
    </define-parameter>
  </define-fault-tree>
</opsa-mef>
