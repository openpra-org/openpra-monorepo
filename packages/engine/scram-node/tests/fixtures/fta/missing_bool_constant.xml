<?xml version="1.0"?>
<!--
This input describes a house event that doesn't have Boolean constant.
This input is valid input for analysis without probability calculations.
However, if the state is not defined, it defaults to false.
-->
<opsa-mef>
  <define-fault-tree name="TwoTrains">
    <define-gate name="TopEvent">
      <and>
        <event name="TrainOne"/>
        <event name="TrainTwo"/>
      </and>
    </define-gate>
    <define-gate name="TrainOne">
      <or>
        <event name="ValveOne"/>
        <event name="PumpOne"/>
      </or>
    </define-gate>
    <define-gate name="TrainTwo">
      <or>
        <event name="ValveTwo"/>
        <event name="PumpTwo"/>
      </or>
    </define-gate>
    <define-house-event name="ValveOne"/>  <!-- No expression. -->
    <define-basic-event name="ValveTwo">
      <float value="0.5"/>
    </define-basic-event>
    <define-basic-event name="PumpOne">
      <float value="0.6"/>
    </define-basic-event>
    <define-basic-event name="PumpTwo">
      <float value="0.7"/>
    </define-basic-event>
  </define-fault-tree>
</opsa-mef>
