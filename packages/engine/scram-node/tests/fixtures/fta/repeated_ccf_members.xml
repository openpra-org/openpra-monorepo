<?xml version="1.0"?>
<!--
The members of CCF group are added twice.
-->
<opsa-mef>
  <define-fault-tree name="TwoTrains">
    <define-gate name="TopEvent">
      <and>
        <event name="TrainOne"/>
        <event name="TrainTwo"/>
      </and>
    </define-gate>
    <define-gate name="TrainOne">
      <or>
        <event name="ValveOne"/>
        <event name="PumpOne"/>
      </or>
    </define-gate>
    <define-gate name="TrainTwo">
      <or>
        <event name="ValveTwo"/>
        <event name="PumpTwo"/>
      </or>
    </define-gate>
    <define-CCF-group name="Pumps" model="MGL">
      <members>
        <basic-event name="PumpOne"/>
        <basic-event name="PumpOne"/>
      </members>
      <distribution>
        <float value="0.1"/>
      </distribution>
      <factor level="2">
        <float value="0.2"/>
      </factor>
    </define-CCF-group>
    <define-basic-event name="ValveOne">
      <float value="0.4"/>
    </define-basic-event>
    <define-basic-event name="ValveTwo">
      <float value="0.5"/>
    </define-basic-event>
    <define-basic-event name="PumpTwo">
      <float value="0.7"/>
    </define-basic-event>
  </define-fault-tree>
</opsa-mef>
