{
  "name": "mef-types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/mef-types/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/mef-types",
        "main": "packages/mef-types/src/index.ts",
        "tsConfig": "packages/mef-types/tsconfig.lib.json",
        "assets": ["packages/mef-types/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "eslintConfig": "packages/mef-types/eslint.config.mjs",
        "lintFilePatterns": ["{projectRoot}/**/*.ts", "!{projectRoot}/**/*.d.ts"]
      }
    },
    "lint-canary": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter mef-types run lint:canary",
        "cwd": "{workspaceRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/mef-types/jest.config.ts"
      }
    }
  }
}
