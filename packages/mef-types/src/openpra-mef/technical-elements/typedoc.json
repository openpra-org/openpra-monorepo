{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./index.ts"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "theme": "default",
  "plugin": ["./.typedoc-plugin.mjs"],
  "name": "Technical Elements Documentation",
  "cleanOutputDir": false,
  
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "hideGenerator": true,
  
  "categorizeByGroup": true,
  
  "navigation": {
    "includeGroups": true,
    "includeCategories": true,
    "includeFolders": false
  },
  
  "sort": ["alphabetical"],
  "disableSources": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "excludeNotDocumented": true,
  
  "groupOrder": [
    "technical_elements",
    "technical_elements.core",
    "technical_elements.integration",
    "*"
  ],
  
  "categoryOrder": [
    "technical_elements",
    "technical_elements.core",
    "technical_elements.integration",
    "*"
  ],
  
  "blockTags": [
    "@remarks", 
    "@returns", 
    "@implements", 
    "@description", 
    "@todo",
    "@group",
    "@example",
    "@extends",
    "@memberof",
    "@param",
    "@const",
    "@see",
    "@minimum",
    "@module",
    "@format",
    "@default"
  ],
  
  "validation": {
    "invalidLink": true,
    "notDocumented": false
  }
}