{
  "name": "model-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/model-generator/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/model-generator",
        "main": "packages/model-generator/src/index.ts",
        "tsConfig": "packages/model-generator/tsconfig.lib.json",
        "assets": ["packages/model-generator/*.md"],
        "transformers": []
      }
    },
    "lint-canary": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter model-generator run lint:canary",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.{ts,js}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "eslintConfig": "packages/model-generator/eslint.config.mjs",
        "jestConfig": "packages/model-generator/jest.config.ts"
      }
    }
  }
}
