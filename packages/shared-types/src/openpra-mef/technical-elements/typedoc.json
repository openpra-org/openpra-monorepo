{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./index.ts"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "theme": "default",
  "plugin": [],
  "name": "Technical Elements Documentation",
  "cleanOutputDir": true,
  
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "hideGenerator": true,
  
  "categorizeByGroup": true,
  
  "navigation": {
    "includeGroups": true,
    "includeCategories": true,
    "includeFolders": false
  },
  
  "sort": ["alphabetical"],
  "disableSources": true,
  
  "groupOrder": [
    "technical_elements",
    "technical_elements.core",
    "*"
  ],
  
  "categoryOrder": [
    "technical_elements",
    "technical_elements.core",
    "*"
  ]
}